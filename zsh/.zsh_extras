#!/bin/zsh

# do this before the rests to align the paths and general
for file in ~/.{functions,alias,path,exports,zsh_functions,zsh_alias,zsh_exports,docker_functions,docker_alias}; do
	if [[ -r "$file" ]] && [[ -f "$file" ]]; then
		# shellcheck source=/dev/null
		source "$file"
	fi
done
unset file
